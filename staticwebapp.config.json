{
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "routes": [
    {
      "route": "/",
      "redirect": "/es/",
      "statusCode": 302
    },
    {
      "route": "/images/*",
      "headers": {
        "Cache-Control": "public, max-age=604800"
      }
    }
  ],
  "globalHeaders": {
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "DENY"
  }
}
